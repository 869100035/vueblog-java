is.chart = echarts.init(this.$el, 'macarons')\n        axios.get('/recordsList').then((res)=>{\n\n          var list = res.data.data.recordsMap;\n          var days = res.data.data.daysList;\n          for(var key in list){\n            var sss={name: key,\n              type: 'bar',\n              stack: 'vistors',\n              barWidth: '60%',\n              data: list[key],\n              animationDuration}\n              ser.push(sss)\n          }\n          console.log(ser)\n          this.chart.setOption({\n            tooltip: {\n              trigger: 'axis',\n              axisPointer: { // 坐标轴指示器，坐标轴触发有效\n                type: 'shadow' // 默认为直线，可选为：'line' | 'shadow'\n              }\n            },\n            grid: {\n              top: 10,\n              left: '2%',\n              right: '2%',\n              bottom: '3%',\n              containLabel: true\n            },\n            xAxis: [{\n              type: 'category',\n              // data: ['Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat', 'Sun'],\n              data:days,\n              axisTick: {\n                alignWithLabel: true\n              }\n            }],\n            yAxis: [{\n              type: 'value',\n              axisTick: {\n                show: false\n              }\n            }],\n            series: ser\n            //         [{\n            //   name: 'pageA',\n            //   type: 'bar',\n            //   stack: 'vistors',\n            //   barWidth: '60%',\n            //   data: [79, 52, 200, 334, 390, 330, 220],\n            //   //data: goodMoney,\n            //   animationDuration\n            // }, {\n            //   name: 'pageB',\n            //   type: 'bar',\n            //   stack: 'vistors',\n            //   barWidth: '60%',\n            //   data: [80, 52, 200, 334, 390, 330, 220],\n            //   animationDuration\n            // }, {\n            //   name: 'pageC',\n            //   type: 'bar',\n            //   stack: 'vistors',\n            //   barWidth: '60%',\n            //   data: [30, 52, 200, 334, 390, 330, 220],\n            //   animationDuration\n            // }]\n          })\n        })\n    }\n  },\n  mounted() {\n    this.$nextTick(() => {\n      this.initChart()\n    })\n  },\n  beforeDestroy() {\n    if (!this.chart) {\n      return\n    }\n    this.chart.dispose()\n    this.chart = null\n  }\n}\n\nconst goodMoney = []\n  const ser = []\n</script>\n"],"sourceRoot":"src/views/TodoList"}]}